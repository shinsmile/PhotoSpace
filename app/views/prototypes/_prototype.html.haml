.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag(prototype.set_main_thumbnail), prototype_path(prototype)
    %button#dropdownMenu.btn.btn-default.dropdown-toggle{"aria-expanded" => "false", "data-toggle" => "dropdown", :type => "button"} ACTION
    %ul.dropdown-menu{"aria-labelledby" => "dropdownMenu1"}
      %li
        %a{"data-method" => "delete", :href => "/prototypes/#{prototype.id}"} Delete
      %li
        %a{:href => "/prototypes/#{prototype.id}/edit"} Edit

    .caption
      %h3
        = prototype.title
